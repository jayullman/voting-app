{
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "strict": [
      2,
      "never"
    ],
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2,
    "no-console": 0,
    "class-methods-use-this": 0,
    "comma-dangle": 0,
    "no-trailing-spaces": 0
  },
  "plugins": [
    "react"
  ],
  "extends": [
    "airbnb-base"
  ]
}
